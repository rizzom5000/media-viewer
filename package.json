{
  "name": "media-viewer",
  "description": "A simple media viewer server written with Express and React.",
  "version": "0.0.1",
  "keywords": [
    "Express",
    "React"
  ],
  "author": "Andrew Miller",
  "license": "MIT",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node --harmony app/app.js",
    "dev": "webpack-dev-server --hot --inline --progress --colors",
    "watch": "webpack --watch --colors --progress --profile",
    "build": "webpack --colors --progress --profile",
    "lint": "eslint src/ app/ -c .eslintrc",
    "test": "node --harmony node_modules/jasmine-node/bin/jasmine-node app/tests && karma start karma.conf",
    "test-server": "node --harmony node_modules/jasmine-node/bin/jasmine-node app/tests",
    "test-client": "karma start karma.conf"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.0",
    "babel-loader": "^5.3.2",
    "browser-request": "^0.3.3",
    "css-loader": "^0.16.0",
    "eslint": "^1.3.1",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-jasmine": "^1.3.1",
    "eslint-plugin-react": "^3.3.0",
    "html-webpack-plugin": "^1.6.1",
    "istanbul-instrumenter-loader": "^0.1.3",
    "jasmine-core": "^2.3.4",
    "jasmine-node": "^1.14.5",
    "karma": "^0.13.9",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.13.3",
    "react-component-gallery": "^1.2.1",
    "react-imageloader": "^2.1.0",
    "react-responsive-grid": "^0.2.0",
    "react-router": "^0.13.3",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  },
  "dependencies": {
    "bluebird": "^2.9.34",
    "elemental": "^0.4.2",
    "express": "^4.13.3",
    "request": "^2.61.0",
    "serve-favicon": "^2.3.0",
    "tingodb": "^0.3.5",
    "winston": "^1.0.1"
  }
}
